generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(cuid())
  email    String  @unique
  firstname String?
  password String
  isResettingPassword Boolean @default(false)
  resetPasswordToken String? @unique
}
